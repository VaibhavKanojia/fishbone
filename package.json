{
	"name": "fishbone",
	"displayName": "fishbone",
	"description": "Create interactive fishbone diagrams",
	"version": "0.0.3",
	"license": "CC-BY-NC-SA-4.0",
	"publisher": "mbehr1",
	"author": {
		"name": "Matthias Behr",
		"email": "mbehr+fishbone@mcbehr.de"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mbehr1/fishbone.git"
	},
	"bugs": {
		"url": "https://github.com/mbehr1/fishbone/issues"
	},
	"homepage": "https://mbehr1.github.io/fishbone/",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:fishbone.fba"
	],
	"main": "./out/extension/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "fishbone.fba",
				"displayName": "Fishbone Analysis",
				"selector": [
					{
						"filenamePattern": "*.fba"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./ && npm run build:webview",
		"build:webview": "yarn --cwd ./src/webview build",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"watch:webview": "webpack --watch --mode development",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/js-yaml": "^3.12.5",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/node-fetch": "^2.5.7",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^7.11.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"js-yaml": "^3.14.0",
		"jsonpath": "^1.0.2",
		"node-fetch": "^2.6.1"
	}
}
